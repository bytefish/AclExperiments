﻿name: "folder"

relation { name: "parent" }

relation { name: "owner" }

relation {
  name: "editor"
  userset_rewrite {
    union {
      child { _this {} }
      child { computed_userset { relation: "owner" } }
}}}

relation {
  name: "viewer"
  userset_rewrite {
  union {
    child { _this {} }
    child { computed_userset { relation: "editor" } }
}}}